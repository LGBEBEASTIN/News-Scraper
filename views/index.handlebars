<div>
    <h1 class="card z-depth-3 center-align">Read the Latest from 'Real GM'</h1>
    <div class="row">
        <h5 class="card">Click 'Scrape' to get the latest articles</h5>
    </div>
</div>
<div class="row">
    <div class="col">
        
        {{#each article}}
        <div class="card">
            <h4>
                {{this.title}}
            </h4>
            <a href="/readArticle/{{this._id}}" class="btn">Read Article Here</a>
        </div>
        {{/each}}
    </div>
</div>
